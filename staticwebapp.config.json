{
    "routes": [
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/*.{png,jpg,gif,ico}",
            "static/css/*"
        ]
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}